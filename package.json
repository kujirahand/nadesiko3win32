{
  "name": "nadesiko3",
  "version": "3.0.48",
  "description": "Japanese Programming Language",
  "main": "src/index.js",
  "bin": {
    "cnako3": "src/cnako3.js",
    "nako3server": "src/nako3server.js",
    "nako3edit": "tools/nako3edit/run.js"
  },
  "scripts": {
    "test": "mocha",
    "start": "node src/nako3server.js",
    "server": "lite-server --config=demo/bs-config.json",
    "nako3edit": "cnako3 tools/nako3edit/index.nako3",
    "nako3edit:run": "node tools/nako3edit/run.js",
    "electron": "electron src/enako3.js",
    "build": "webpack --mode production",
    "build:command": "cnako3 batch/build_command.nako3",
    "build:electron": "asar pack src/enako3.js release/enako3.asar",
    "build:win32": "cnako3 installer/make-win32.nako3",
    "watch": "webpack --watch --mode development"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/kujirahand/nadesiko3.git"
  },
  "keywords": [
    "script",
    "Japanse Programming Language",
    "Nadesiko",
    "日本語プログラミング言語",
    "なでしこ"
  ],
  "files": [
    "bin",
    "demo",
    "doc",
    "src",
    "test",
    "tools",
    "release"
  ],
  "author": "kujirahand",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/kujirahand/nadesiko3/issues"
  },
  "babel": {
    "presets": [
      [
        "env",
        {
          "targets": {
            "browsers": [
              "> 0.5%",
              "> 0.5% in JP",
              "last 2 versions",
              "Firefox ESR",
              "not dead"
            ],
            "useBuiltIns": true
          }
        }
      ],
      "react"
    ]
  },
  "eslintConfig": {
    "extends": [
      "standard",
      "eslint:recommended",
      "plugin:react/recommended"
    ],
    "env": {
      "mocha": true,
      "browser": true
    },
    "rules": {
      "no-console": "off"
    },
    "plugins": [
      "react"
    ]
  },
  "homepage": "https://github.com/kujirahand/nadesiko3#readme",
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.3",
    "babel-loader": "^7.1.5",
    "babel-polyfill": "^6.26.0",
    "babel-preset-env": "^1.7.0",
    "babel-preset-react": "^6.23.0",
    "date-utils": "^1.2.21",
    "es6-promise": "^4.2.4",
    "eslint": "^4.19.1",
    "eslint-config-standard": "^11.0.0",
    "eslint-plugin-import": "^2.13.0",
    "eslint-plugin-node": "^6.0.1",
    "eslint-plugin-promise": "^3.8.0",
    "eslint-plugin-react": "^7.10.0",
    "eslint-plugin-standard": "^3.1.0",
    "isomorphic-fetch": "^2.2.1",
    "lite-server": "^2.4.0",
    "mocha": "^5.2.0",
    "nadesiko3-hoge": "0.0.2",
    "prop-types": "^15.6.2",
    "pump": "^3.0.0",
    "source-map": "^0.7.3",
    "uuid": "^3.3.2",
    "webpack": "^4.16.5",
    "webpack-cli": "^2.1.5"
  },
  "dependencies": {
    "body-parser": "^1.18.3",
    "commander": "^2.17.1",
    "copy-paste": "^1.3.0",
    "express": "^4.16.3",
    "form-data": "^2.3.2",
    "fs-extra": "^6.0.0",
    "iconv-lite": "^0.4.23",
    "moment-timezone": "^0.5.21",
    "node-fetch": "^2.2.0",
    "npm": "^6.3.0",
    "opener": "^1.4.3",
    "react": "^16.4.2",
    "react-dom": "^16.4.2",
    "ssri": "^6.0.0",
    "whatwg-fetch": "^2.0.4"
  }
}
